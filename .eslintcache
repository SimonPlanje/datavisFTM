city\", 1)\r\n                .style('fill', 'white')\r\n                .style('stroke', d => gScale(d.male))\r\n                .style('stroke-opacity', 0.1)\r\n                .style('stroke-width', d => rScale(d.avarageImpress))\r\n\r\n                g.selectAll('circles').data(data)\r\n                .exit().remove()\r\n\r\n\r\n        }\r\n        else if(filterData === \"age\"){\r\n            g.selectAll('circles').data(data)\r\n            .enter().append('circle')\r\n                .attr('cx', d => xScale(d.ad_delivery_start_time))\r\n                .attr('cy', d => yScale(d.advertiser_name))\r\n                .attr('r', 2)\r\n                .style(\"opacity\", 1)\r\n                .style('fill', 'white')\r\n                .style('stroke', d => aScale(d.thirdy))\r\n                .style('stroke-opacity', 0.2)\r\n                .style('stroke-width', d => rScale(d.avarageImpress))\r\n        }\r\n    }\r\n\r\n\r\n    const yAxis = axisLeft(yScale)\r\n        .tickSize(-innerWidth)\r\n\r\n    const yAxisG = g\r\n        .append('g')\r\n        .attr(\"class\", \"yScale\")\r\n        .attr('transform', `translate(${0},${0})`)\r\n        .call(yAxis)\r\n\r\n        yAxisG\r\n        .selectAll('.domain').remove()\r\n\r\n           render(); \r\n       \r\n       return(\r\n            <div className='d3div'>\r\n                <svg className=\"SVG\" width={width} height={height}>\r\n                    <g></g>\r\n                </svg>\r\n            </div>\r\n        )\r\n    } else{\r\n        return null\r\n    }\r\n}\r\n\r\nexport default CreateVis","D:\\school\\datavisFTM\\src\\components\\Header.js",["53"],"\r\nfunction Header(){\r\n     \r\n    return(\r\n        <div className=\"div\">\r\n            <img className='headerImg' src=\"https://www.ftm.nl/uploads/cache/article_2800_nocrop/uploads/media/5fc527483804e/politicaladsdatabase-v1.jpg\" alt=\"politics header image\"/>\r\n          <h1>Welke doelgroepen proberen de politieke partijen te b